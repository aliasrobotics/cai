+
+
+def send_email(recipient, subject, body):
+    print("Sending email...")
+    print(f"To: {recipient}")
+    print(f"Subject: {subject}")
+    print(f"Body: {body}")
+    return "Sent!"
+
+
+weather_agent = Agent(
+    name="Weather Agent",
+    instructions="You are a helpful agent.",
+    functions=[get_weather, send_email],
+)
